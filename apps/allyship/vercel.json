{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "yarn turbo build",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/cartoons/_nuxt/(.*)",
      "destination": "https://accessibility-is.vercel.app/cartoons/_nuxt/$1"
    },
    {
      "source": "/cartoons/(.*)",
      "destination": "https://accessibility-is.vercel.app/cartoons/$1"
    },
    {
      "source": "/cartoons",
      "destination": "https://accessibility-is.vercel.app/cartoons/"
    }
  ]
}
