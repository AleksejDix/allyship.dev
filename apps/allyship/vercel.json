{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "yarn turbo build",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/cartoons",
      "destination": "https://accessibility-is.vercel.app/cartoons/"
    },
    {
      "source": "/cartoons/",
      "destination": "https://accessibility-is.vercel.app/cartoons/"
    },
    {
      "source": "/cartoons/:path*",
      "destination": "https://accessibility-is.vercel.app/cartoons/:path*"
    }
  ]
}
